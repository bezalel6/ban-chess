{
  "name": "next-chess",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "diagnose:game": "node scripts/diagnose_game.mjs",
    "kill-running": "node scripts/kill-dev-processes.js",
    "kill-running-old": "portio --mine 3000",
    "build": "next build",
    "build:simple": "next build",
    "check": "npm run lint && tsc --noEmit",
    "dev:local": "npm run kill-running && dotenv -e .env.local -- next dev",
    "supabase:start": "npx dotenv-cli -e .env.local -- supabase start",
    "typegen": "npx supabase gen types --local > src/types/database.ts && cp src/types/database.ts supabase/functions/_shared/database-types.ts",
    "db:remote-reset": "npx supabase@beta db reset --linked",
    "dev": "npm run kill-running && next dev --turbo",
    "dev:watch": "npm run kill-running && next dev",
    "dev:safe": "npm run kill-running && next dev",
    "dev:safe:watch": "npm run kill-running && next dev",
    "build:server:dev": "echo \"(disabled) using built-in Next dev server.\"",
    "dev:ultra": "npm run kill-running && cross-env NEXT_CACHE_DIR=\"$LOCALAPPDATA/next-cache\" NEXT_TELEMETRY_DISABLED=1 TSX_NO_SOURCEMAP=1 DOTENV_CONFIG_PATH=.env next dev",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "dotenv -e .env.production -- next build && node .next/standalone/server.js",
    "start": "dotenv -e .env.production -- node .next/standalone/server.js",
    "typecheck": "tsc --noEmit",
    "functions:serve": "supabase functions serve --env-file supabase/functions/.env",
    "functions:secrets:push": "supabase secrets set --env-file supabase/functions/.env",
    "functions:secrets:list": "supabase secrets list",
    "templates:push": "node scripts/push-email-templates.js",
    "templates:check": "node scripts/check-email-templates.js",
    "config:push": "node scripts/push-remote-config.js",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@eslint/eslintrc": "^3.0.0",
    "@marsidev/react-turnstile": "^1.3.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
