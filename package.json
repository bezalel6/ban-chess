{
  "name": "next-chess",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "kill-running": "portio --mine 3000",
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev:local": "npm run kill-running && cross-env DOTENV_CONFIG_PATH=.env.local tsx -r dotenv/config src/server/server.ts",
    "supabase:start": "npx dotenv-cli -e .env.local -- supabase start",
    "typegen": "npx supabase gen types --local > src/types/database.ts && cp src/types/database.ts supabase/functions/_shared/database-types.ts",
    "db:remote-reset": "npx supabase@beta db reset --linked",
    "dev": "npm run kill-running && tsx -r dotenv/config src/server/server.ts",
    "dev:watch": "npm run kill-running && tsx --watch -r dotenv/config src/server/server.ts",
    "build:server:dev": "echo \"(disabled) esbuild bundling is incompatible with Next internals in dev; using tsx watch instead.\"",
    "dev:ultra": "npm run kill-running && cross-env NEXT_TELEMETRY_DISABLED=1 TSX_NO_SOURCEMAP=1 DOTENV_CONFIG_PATH=.env tsx --watch -r dotenv/config src/server/server.ts",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "tsx -r dotenv/config src/server/server.ts",
    "typecheck": "tsc --noEmit",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug --headed",
    "test:cleanup": "node tests/cleanup-processes.js",
    "test:install": "playwright install",
    "functions:serve": "supabase functions serve --env-file supabase/functions/.env",
    "functions:secrets:push": "supabase functions secrets set --env-file supabase/functions/.env",
    "functions:secrets:list": "supabase functions secrets list"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@eslint/eslintrc": "^3.0.0",
    "@marsidev/react-turnstile": "^1.3.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@prisma/client": "^6.5.0",
    "@react-chess/chessground": "^1.3.4",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.39.3",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.84.2",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@types/estree": "^1.0.7",
    "@types/json-schema": "^7.0.15",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^10.0.0",
    "@uploadthing/react": "^7.3.2",
    "bad-words": "^4.0.0",
    "chart.js": "^4.5.0",
    "chess.js": "^1.4.0",
    "chess.ts": "^0.16.2",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.6",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.0",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "next": "^15.4.6",
    "nodemon": "^3.0.3",
    "nuqs": "^2.4.3",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-chessboard": "^4.7.3",
    "react-dom": "^19.1.1",
    "react-image-crop": "^11.0.10",
    "react-intersection-observer": "^9.16.0",
    "the-big-username-blacklist": "^1.5.2",
    "ts-node": "^10.9.2",
    "uploadthing": "^7.7.3",
    "use-sound": "^5.0.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.0",
    "esbuild": "^0.24.2",
    "prettier": "^3.2.5",
    "tsx": "^4.20.4",
    "typescript": "^5.3.3",
