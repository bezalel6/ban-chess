    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "dotenv -e ./.env -- npx tsx src/server/server.ts",
    "typecheck": "tsc --noEmit",
    "test:e2e": "jest --config=e2e/jest.config.js",
    "test:e2e:debug": "HEADLESS=false SLOW_MO=100 jest --config=e2e/jest.config.js",
