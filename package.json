    "@tanstack/react-query": "^5.84.2",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@types/estree": "^1.0.7",
    "@types/json-schema": "^7.0.15",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.11.19",
    "@types/uuid": "^10.0.0",
    "@uploadthing/react": "^7.3.2",
    "bad-words": "^4.0.0",
    "ban-chess.ts": "^1.1.1",
    "chart.js": "^4.5.0",
    "chessground": "^9.2.1",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.6",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "dotenv-cli": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.0",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "next": "^15.4.6",
    "nodemon": "^3.0.3",
    "nuqs": "^2.4.3",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-chessboard": "^4.7.3",
    "react-dom": "^19.1.1",
    "react-image-crop": "^11.0.10",
    "react-intersection-observer": "^9.16.0",
    "the-big-username-blacklist": "^1.5.2",
    "ts-node": "^10.9.2",
    "uploadthing": "^7.7.3",
    "use-sound": "^5.0.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4",
    "zustand": "^5.0.7"
  },
